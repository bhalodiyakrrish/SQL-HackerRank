SELECT h.hacker_id, name
FROM Hackers h
INNER JOIN
(SELECT s.hacker_id, COUNT(s.submission_id) AS c_id
FROM Submissions s
LEFT JOIN Challenges c
ON s.challenge_id = c.challenge_id
LEFT JOIN Difficulty d
ON c.difficulty_level = d.difficulty_level AND s.score = d.score
WHERE s.score = d.score
GROUP BY s.hacker_id
HAVING COUNT(s.submission_id) > 1) sub
ON h.hacker_id = sub.hacker_id
ORDER BY sub.c_id DESC, h.hacker_id ASC;

